  "tokenColors": [
    { "scope": ["keyword"], "settings": { "foreground": "#FF3C00", "fontStyle": "bold" } },
    { "scope": ["entity.name.function"], "settings": { "foreground": "#FFD700", "fontStyle": "italic" } },
    { "scope": ["variable"], "settings": { "foreground": "#FFE6C7" } },
    { "scope": ["string"], "settings": { "foreground": "#FFD700" } },
    { "scope": ["comment"], "settings": { "foreground": "#A06B5C", "fontStyle": "italic" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#FFD700" } },
    { "scope": ["storage.type"], "settings": { "foreground": "#FF3C00", "fontStyle": "bold" } },
    { "scope": ["entity.name.namespace.go"], "settings": { "foreground": "#FFD700", "fontStyle": "bold" } },
    { "scope": ["entity.name.import.go"], "settings": { "foreground": "#FF3C00", "fontStyle": "bold" } },
    { "scope": ["variable.other.package.go"], "settings": { "foreground": "#FF3C00", "fontStyle": "bold" } },
    { "scope": ["entity.name.import.path.go"], "settings": { "foreground": "#FFD700", "fontStyle": "bold underline" } }
