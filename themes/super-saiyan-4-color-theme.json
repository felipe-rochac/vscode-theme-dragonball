    "sideBarSectionHeader.foreground": "#FFD700",
    "statusBar.background": "#FF3C00",
    "statusBar.foreground": "#1A1010",
    "statusBar.noFolderBackground": "#2C1818",
    "statusBar.debuggingBackground": "#FFD700",
    "statusBar.debuggingForeground": "#1A1010",
    "titleBar.activeBackground": "#FF3C00",
    "titleBar.activeForeground": "#FFE6C7",
    "titleBar.inactiveBackground": "#2C1818",
    "titleBar.inactiveForeground": "#FFE6C799",
    "tab.activeBackground": "#FF3C00",
    "tab.activeForeground": "#FFD700",
    "tab.inactiveBackground": "#2C1818",
    "tab.inactiveForeground": "#FFE6C799",
    "tab.border": "#FFD700",
    "tab.activeBorder": "#FFD700",
    "tab.unfocusedActiveBorder": "#FFD70099",
    "panel.background": "#2C1818",
    "panel.border": "#FFD700",
    "editorGroupHeader.tabsBackground": "#2C1818"
