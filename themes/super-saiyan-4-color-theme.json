    "editorIndentGuide.background1": "#2C1818",
    "editorIndentGuide.activeBackground1": "#FF3C00",
    "editorLineNumber.foreground": "#A06B5C",
    "editorLineNumber.activeForeground": "#FF3C00",
    "activityBar.background": "#FF3C00",
    "activityBar.foreground": "#FFE6C7",
    "activityBar.activeBorder": "#FFD700",
    "activityBar.inactiveForeground": "#FFE6C799",
    "activityBarBadge.background": "#FFD700",
