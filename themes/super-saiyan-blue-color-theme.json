{
      "name": "Dragon Ball Super Saiyan Blue",
      "type": "dark",
      "tokenColors": [
        { "scope": ["comment", "comment.line", "comment.block"], "settings": { "foreground": "#7FCFFF", "fontStyle": "italic" } },
        { "scope": ["keyword", "keyword.control.go"], "settings": { "foreground": "#00BFFF", "fontStyle": "bold" } }, // Blue
        { "scope": ["entity.name.function", "entity.name.function.go"], "settings": { "foreground": "#00FFFF", "fontStyle": "italic" } }, // Cyan
        { "scope": ["variable", "variable.other.go"], "settings": { "foreground": "#E0FFFF" } }, // Pale blue
        { "scope": ["string", "string.quoted.double.go", "string.quoted.single.go"], "settings": { "foreground": "#00BFFF" } },
        { "scope": ["constant.numeric", "constant.numeric.go"], "settings": { "foreground": "#00FFFF" } }, // Cyan
        { "scope": ["storage.type", "storage.type.go"], "settings": { "foreground": "#00BFFF", "fontStyle": "bold" } }, // Blue
        { "scope": ["entity.name.namespace.go"], "settings": { "foreground": "#00BFFF", "fontStyle": "bold" } }, // Cyan
        { "scope": ["entity.name.import.go", "variable.other.package.go"], "settings": { "foreground": "#15304A", "fontStyle": "bold" } },
        { "scope": ["entity.name.import.path.go"], "settings": { "foreground": "#FFD700", "fontStyle": "bold underline" } },
        { "scope": ["entity.name.type", "entity.name.type.go"], "settings": { "foreground": "#FFD700", "fontStyle": "bold" } },
        { "scope": ["source.go"], "settings": { "foreground": "#E0FFFF" } }
      ],
      "colors": {
        // GitHub Dark base colors
        "editor.background": "#0d1117",
        "editor.foreground": "#E0FFFF",
        "editorCursor.foreground": "#00FFFF",
        "editor.lineHighlightBackground": "#161b22",
        "editor.selectionBackground": "#264f78",
        "editor.selectionHighlightBackground": "#3b3b3b60",
        "editorIndentGuide.background1": "#21262d",
        "editorIndentGuide.activeBackground1": "#00FFFF",
        "editorLineNumber.foreground": "#7FCFFF",
        "editorLineNumber.activeForeground": "#00FFFF",
        "activityBar.background": "#161b22",
        "activityBar.foreground": "#E0FFFF",
        "activityBar.activeBorder": "#00BFFF",
        "activityBar.inactiveForeground": "#6e7681",
        "activityBarBadge.background": "#00BFFF",
        "activityBarBadge.foreground": "#161b22",
        "sideBar.background": "#161b22",
        "sideBar.foreground": "#E0FFFF",
        "sideBarSectionHeader.background": "#21262d",
        "sideBarSectionHeader.foreground": "#00BFFF",
        "statusBar.background": "#161b22",
        "statusBar.foreground": "#E0FFFF",
        "statusBar.noFolderBackground": "#161b22",
        "statusBar.debuggingBackground": "#00FFFF",
        "statusBar.debuggingForeground": "#161b22",
        "titleBar.activeBackground": "#161b22",
        "titleBar.activeForeground": "#E0FFFF",
        "titleBar.inactiveBackground": "#0d1117",
        "titleBar.inactiveForeground": "#6e7681",
        "tab.activeBackground": "#21262d",
        "tab.activeForeground": "#00BFFF",
        "tab.inactiveBackground": "#161b22",
        "tab.inactiveForeground": "#7FCFFF",
        "tab.border": "#21262d",
        "tab.activeBorder": "#00BFFF",
        "tab.unfocusedActiveBorder": "#00FFFF99",
        "panel.background": "#161b22",
        "panel.border": "#00BFFF",
        "editorGroupHeader.tabsBackground": "#161b22"
      }
    }