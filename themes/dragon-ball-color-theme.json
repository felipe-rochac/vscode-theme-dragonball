{
  "name": "Dragon Ball",
  "type": "dark",
  "colors": {
    // GitHub Dark base colors
    "editor.background": "#0d1117",
    "editor.foreground": "#FCFCFA",
    "editorCursor.foreground": "#FF9800",
    "editor.lineHighlightBackground": "#161b22",
    "editor.selectionBackground": "#264f78",
    "editor.selectionHighlightBackground": "#3b3b3b60",
    "editorIndentGuide.background1": "#21262d",
    "editorIndentGuide.activeBackground1": "#1976D2",
    "editorLineNumber.foreground": "#FF9800",
    "editorLineNumber.activeForeground": "#1976D2",
    "activityBar.background": "#161b22",
    "activityBar.foreground": "#FCFCFA",
    "activityBar.activeBorder": "#FF9800",
    "activityBar.inactiveForeground": "#6e7681",
    "activityBarBadge.background": "#FF9800",
    "activityBarBadge.foreground": "#161b22",
    "sideBar.background": "#161b22",
    "sideBar.foreground": "#FCFCFA",
    "sideBarSectionHeader.background": "#21262d",
    "sideBarSectionHeader.foreground": "#FF9800",
    "statusBar.background": "#161b22",
    "statusBar.foreground": "#FCFCFA",
    "statusBar.noFolderBackground": "#161b22",
    "statusBar.debuggingBackground": "#1976D2",
    "statusBar.debuggingForeground": "#161b22",
    "titleBar.activeBackground": "#161b22",
    "titleBar.activeForeground": "#FCFCFA",
    "titleBar.inactiveBackground": "#0d1117",
    "titleBar.inactiveForeground": "#6e7681",
    "tab.activeBackground": "#21262d",
    "tab.activeForeground": "#FF9800",
    "tab.inactiveBackground": "#161b22",
    "tab.inactiveForeground": "#FF9800",
    "tab.border": "#21262d",
    "tab.activeBorder": "#FF9800",
    "tab.unfocusedActiveBorder": "#1976D299",
    "panel.background": "#161b22",
    "panel.border": "#1976D2",
    "editorGroupHeader.tabsBackground": "#161b22"
  },
  "tokenColors": [
    { "scope": ["comment", "comment.line", "comment.block"], "settings": { "foreground": "#727072", "fontStyle": "italic" } },
    { "scope": ["keyword", "keyword.control.go"], "settings": { "foreground": "#FF9800", "fontStyle": "bold" } },
    { "scope": ["entity.name.function", "entity.name.function.go"], "settings": { "foreground": "#1976D2", "fontStyle": "italic" } },
    { "scope": ["variable", "variable.other.go"], "settings": { "foreground": "#FCFCFA" } },
    { "scope": ["string", "string.quoted.double.go", "string.quoted.single.go"], "settings": { "foreground": "#FF9800" } },
    { "scope": ["constant.numeric", "constant.numeric.go"], "settings": { "foreground": "#1976D2" } },
    { "scope": ["storage.type", "storage.type.go"], "settings": { "foreground": "#FF9800", "fontStyle": "bold" } },
    { "scope": ["entity.name.import.go", "variable.other.package.go"], "settings": { "foreground": "#1976D2", "fontStyle": "bold" } },
    { "scope": ["entity.name.import.path.go"], "settings": { "foreground": "#FF9800", "fontStyle": "bold underline" } },
    { "scope": ["source.go"], "settings": { "foreground": "#FCFCFA" } },
    { "scope": ["entity.name.tag.yaml"], "settings": { "foreground": "#FF9800", "fontStyle": "bold" } },
    { "scope": ["entity.name.anchor.yaml"], "settings": { "foreground": "#1976D2", "fontStyle": "bold underline" } },
    { "scope": ["entity.name.alias.yaml"], "settings": { "foreground": "#FCFCFA", "fontStyle": "bold" } },
    { "scope": ["string.unquoted.plain.out.yaml", "string.quoted.double.yaml", "string.quoted.single.yaml"], "settings": { "foreground": "#FCFCFA" } },
    { "scope": ["constant.numeric.yaml"], "settings": { "foreground": "#1976D2" } },
    { "scope": ["constant.language.boolean.yaml"], "settings": { "foreground": "#FF9800", "fontStyle": "bold" } },
    { "scope": ["constant.language.null.yaml"], "settings": { "foreground": "#727072", "fontStyle": "italic" } },
    { "scope": ["comment.line.number-sign.yaml"], "settings": { "foreground": "#727072", "fontStyle": "italic" } }
  ]
}
