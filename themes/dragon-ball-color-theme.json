{
  "name": "Dragon Ball",
  "type": "dark",
  "colors": {
    "editor.background": "#0d1117",
    "editor.foreground": "#FCFCFA",
    "editorCursor.foreground": "#FF9800",
    "editor.lineHighlightBackground": "#161b22",
    "editor.selectionBackground": "#264f78",
    "editor.selectionHighlightBackground": "#3b3b3b60",
    "editorIndentGuide.background1": "#21262d",
    "editorIndentGuide.activeBackground1": "#1976D2",
    "editorLineNumber.foreground": "#FF9800",
    "editorLineNumber.activeForeground": "#1976D2",
    "activityBar.background": "#161b22",
    "activityBar.foreground": "#FCFCFA",
    "activityBar.activeBorder": "#FF9800",
    "activityBar.inactiveForeground": "#6e7681",
    "activityBarBadge.background": "#FF9800",
    "activityBarBadge.foreground": "#161b22",
    "sideBar.background": "#161b22",
    "sideBar.foreground": "#FCFCFA",
    "sideBarSectionHeader.background": "#21262d",
    "sideBarSectionHeader.foreground": "#FF9800",
    "statusBar.background": "#161b22",
    "statusBar.foreground": "#FCFCFA",
    "statusBar.noFolderBackground": "#161b22",
    "statusBar.debuggingBackground": "#1976D2",
    "statusBar.debuggingForeground": "#161b22",
    "titleBar.activeBackground": "#161b22",
    "titleBar.activeForeground": "#FCFCFA",
    "titleBar.inactiveBackground": "#0d1117",
    "titleBar.inactiveForeground": "#6e7681",
    "tab.activeBackground": "#21262d",
    "tab.activeForeground": "#FF9800",
    "tab.inactiveBackground": "#161b22",
    "tab.inactiveForeground": "#FF9800",
    "tab.border": "#21262d",
    "tab.activeBorder": "#FF9800",
    "tab.unfocusedActiveBorder": "#1976D299",
    "panel.background": "#161b22",
    "panel.border": "#1976D2",
    "editorGroupHeader.tabsBackground": "#161b22"
  },
  "tokenColors": [
    { "scope": ["comment", "comment.line", "comment.block"], "settings": { "foreground": "#727072", "fontStyle": "italic" } },
    { "scope": ["keyword", "keyword.control.go"], "settings": { "foreground": "#FF9800", "fontStyle": "bold" } },
    { "scope": ["entity.name.function", "entity.name.function.go"], "settings": { "foreground": "#1976D2", "fontStyle": "italic" } },
    { "scope": ["variable", "variable.other.go"], "settings": { "foreground": "#FCFCFA" } },
    { "scope": ["string", "string.quoted.double.go", "string.quoted.single.go"], "settings": { "foreground": "#FF9800" } },
    { "scope": ["constant.numeric", "constant.numeric.go"], "settings": { "foreground": "#1976D2" } },
    { "scope": ["storage.type", "storage.type.go"], "settings": { "foreground": "#FF9800", "fontStyle": "bold" } },
    { "scope": ["entity.name.import.go", "variable.other.package.go"], "settings": { "foreground": "#1976D2", "fontStyle": "bold" } },
    { "scope": ["entity.name.import.path.go"], "settings": { "foreground": "#FF9800", "fontStyle": "bold underline" } },
    { "scope": ["source.go"], "settings": { "foreground": "#FCFCFA" } },
    { "scope": ["entity.name.tag.yaml"], "settings": { "foreground": "#FFD700", "fontStyle": "bold" } },
    { "scope": ["entity.name.anchor.yaml"], "settings": { "foreground": "#1976D2", "fontStyle": "bold underline" } },
    { "scope": ["entity.name.alias.yaml"], "settings": { "foreground": "#FFD700", "fontStyle": "bold" } },
    { "scope": ["string.unquoted.plain.out.yaml"], "settings": { "foreground": "#3B4D9B" } },
    { "scope": ["string.quoted.double.yaml", "string.quoted.single.yaml"], "settings": { "foreground": "#1976D2" } },
    { "scope": ["constant.numeric.yaml"], "settings": { "foreground": "#3B4D9B" } },
    { "scope": ["constant.language.boolean.yaml"], "settings": { "foreground": "#FFD700", "fontStyle": "bold" } },
    { "scope": ["constant.language.null.yaml"], "settings": { "foreground": "#6B7CA0", "fontStyle": "italic" } },
    { "scope": ["comment.line.number-sign.yaml"], "settings": { "foreground": "#6B7CA0", "fontStyle": "italic" } },
    { "scope": ["support.type.property-name.json", "meta.object-literal.key.js", "meta.object-literal.key.ts", "meta.object-literal.key.tsx"], "settings": { "foreground": "#FFD700", "fontStyle": "bold" } },
    { "scope": ["string.value.json", "string.quoted.double.js", "string.quoted.single.js", "string.quoted.double.ts", "string.quoted.single.ts", "string.quoted.double.tsx", "string.quoted.single.tsx"], "settings": { "foreground": "#1976D2" } },
    { "scope": ["constant.numeric.json", "constant.numeric.js", "constant.numeric.ts", "constant.numeric.tsx"], "settings": { "foreground": "#1976D2" } },
    { "scope": ["constant.language.boolean.json", "constant.language.boolean.js", "constant.language.boolean.ts", "constant.language.boolean.tsx"], "settings": { "foreground": "#FFD700", "fontStyle": "bold" } },
    { "scope": ["constant.language.null.json", "constant.language.null.js", "constant.language.null.ts", "constant.language.null.tsx"], "settings": { "foreground": "#6B7CA0", "fontStyle": "italic" } },
    { "scope": ["comment.line.double-slash.js", "comment.line.double-slash.ts", "comment.line.double-slash.tsx", "comment.block.documentation.js", "comment.block.documentation.ts", "comment.block.documentation.tsx"], "settings": { "foreground": "#6B7CA0", "fontStyle": "italic" } },
    { "scope": ["punctuation.separator.key-value.json", "punctuation.separator.key-value.js", "punctuation.separator.key-value.ts", "punctuation.separator.key-value.tsx"], "settings": { "foreground": "#FFD700" } },
    { "scope": ["punctuation.definition.block.js", "punctuation.definition.block.ts", "punctuation.definition.block.tsx"], "settings": { "foreground": "#1976D2" } },
    { "scope": ["keyword.control.makefile"], "settings": { "foreground": "#FFD700", "fontStyle": "bold" } },
    { "scope": ["variable.assignment.makefile"], "settings": { "foreground": "#1976D2", "fontStyle": "bold" } },
    { "scope": ["entity.name.function.target.makefile"], "settings": { "foreground": "#FFD700", "fontStyle": "bold underline" } },
    { "scope": ["string.quoted.double.makefile", "string.quoted.single.makefile"], "settings": { "foreground": "#1976D2" } },
    { "scope": ["comment.line.number-sign.makefile"], "settings": { "foreground": "#6B7CA0", "fontStyle": "italic" } },
    { "scope": ["meta.scope.target.makefile"], "settings": { "foreground": "#FFD700", "fontStyle": "bold" } },
    { "scope": ["meta.scope.prerequisites.makefile"], "settings": { "foreground": "#FFD700", "fontStyle": "" } },
    { "scope": ["entity.name.type", "entity.name.type.go"], "settings": { "foreground": "#FF9800", "fontStyle": "bold" } },
    // Terraform
    { "scope": ["keyword.control.terraform"], "settings": { "foreground": "#FF9800", "fontStyle": "bold" } },
    { "scope": ["variable.other.terraform"], "settings": { "foreground": "#FCFCFA" } },
    { "scope": ["entity.name.function.terraform"], "settings": { "foreground": "#1976D2", "fontStyle": "italic" } },
    { "scope": ["entity.name.type.terraform"], "settings": { "foreground": "#FF9800", "fontStyle": "bold" } },
    { "scope": ["string.quoted.double.terraform", "string.quoted.single.terraform"], "settings": { "foreground": "#1976D2" } },
    { "scope": ["constant.numeric.terraform"], "settings": { "foreground": "#1976D2" } },
    { "scope": ["constant.language.boolean.terraform"], "settings": { "foreground": "#FF9800", "fontStyle": "bold" } },
    { "scope": ["comment.line.number-sign.terraform"], "settings": { "foreground": "#727072", "fontStyle": "italic" } },
    { "scope": ["support.constant.terraform"], "settings": { "foreground": "#FFD700", "fontStyle": "bold" } },
    { "scope": ["punctuation.definition.block.terraform"], "settings": { "foreground": "#FF9800" } },
    { "scope": ["support.function.builtin.terraform"], "settings": { "foreground": "#FF9800", "fontStyle": "bold italic" } }
  ]
}
