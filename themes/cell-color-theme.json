{
  "name": "Dragon Ball Cell",
  "type": "dark",
  "colors": {
    // GitHub Dark base colors
    "editor.background": "#0d1117",
    "editor.foreground": "#E0FFD6",
    "editorCursor.foreground": "#7FFF00",
    "editor.lineHighlightBackground": "#161b22",
    "editor.selectionBackground": "#264f78",
    "editor.selectionHighlightBackground": "#3b3b3b60",
    "editorIndentGuide.background1": "#21262d",
    "editorIndentGuide.activeBackground1": "#7FFF00",
    "editorLineNumber.foreground": "#5C995C",
    "editorLineNumber.activeForeground": "#7FFF00",
    "activityBar.background": "#161b22",
    "activityBar.foreground": "#E0FFD6",
    "activityBar.activeBorder": "#32CD32",
    "activityBar.inactiveForeground": "#6e7681",
    "activityBarBadge.background": "#32CD32",
    "activityBarBadge.foreground": "#161b22",
    "sideBar.background": "#161b22",
    "sideBar.foreground": "#E0FFD6",
    "sideBarSectionHeader.background": "#21262d",
    "sideBarSectionHeader.foreground": "#32CD32",
    "statusBar.background": "#161b22",
    "statusBar.foreground": "#E0FFD6",
    "statusBar.noFolderBackground": "#161b22",
    "statusBar.debuggingBackground": "#7FFF00",
    "statusBar.debuggingForeground": "#161b22",
    "titleBar.activeBackground": "#161b22",
    "titleBar.activeForeground": "#E0FFD6",
    "titleBar.inactiveBackground": "#0d1117",
    "titleBar.inactiveForeground": "#6e7681",
    "tab.activeBackground": "#21262d",
    "tab.activeForeground": "#7FFF00",
    "tab.inactiveBackground": "#161b22",
    "tab.inactiveForeground": "#5C995C",
    "tab.border": "#21262d",
    "tab.activeBorder": "#32CD32",
    "tab.unfocusedActiveBorder": "#7FFF0099",
    "panel.background": "#161b22",
    "panel.border": "#32CD32",
    "editorGroupHeader.tabsBackground": "#161b22"
  },
  "tokenColors": [
    { "scope": ["comment", "comment.line", "comment.block"], "settings": { "foreground": "#5C995C", "fontStyle": "italic" } },
    { "scope": ["keyword", "keyword.control.go"], "settings": { "foreground": "#32CD32", "fontStyle": "bold" } },
    { "scope": ["entity.name.function", "entity.name.function.go"], "settings": { "foreground": "#7FFF00", "fontStyle": "italic" } },
    { "scope": ["variable", "variable.other.go"], "settings": { "foreground": "#E0FFD6" } },
    { "scope": ["string", "string.quoted.double.go", "string.quoted.single.go"], "settings": { "foreground": "#32CD32" } },
    { "scope": ["constant.numeric", "constant.numeric.go"], "settings": { "foreground": "#FFD700" } },
    { "scope": ["storage.type", "storage.type.go"], "settings": { "foreground": "#32CD32", "fontStyle": "bold" } },
    { "scope": ["entity.name.import.go", "variable.other.package.go"], "settings": { "foreground": "#5C995C", "fontStyle": "bold" } },
    { "scope": ["entity.name.import.path.go"], "settings": { "foreground": "#FFD700", "fontStyle": "bold underline" } },
    { "scope": ["source.go"], "settings": { "foreground": "#E0FFD6" } }
  ]
}
