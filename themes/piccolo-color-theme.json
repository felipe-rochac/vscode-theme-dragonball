    "statusBar.foreground": "#101B10",
    "statusBar.noFolderBackground": "#183218",
    "statusBar.debuggingBackground": "#A98BFF",
    "statusBar.debuggingForeground": "#101B10",
    "titleBar.activeBackground": "#7FFF00",
    "titleBar.activeForeground": "#DFFFD0",
    "titleBar.inactiveBackground": "#183218",
    "titleBar.inactiveForeground": "#DFFFD099",
    "tab.activeBackground": "#7FFF00",
    "tab.activeForeground": "#A98BFF",
    "tab.inactiveBackground": "#183218",
    "tab.inactiveForeground": "#DFFFD099",
    "tab.border": "#A98BFF",
    "tab.activeBorder": "#A98BFF",
    "tab.unfocusedActiveBorder": "#A98BFF99",
    "panel.background": "#0D180D",
    "panel.border": "#A98BFF",
