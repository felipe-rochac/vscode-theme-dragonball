  "tokenColors": [
    { "scope": ["keyword"], "settings": { "foreground": "#7FFF00", "fontStyle": "bold" } },
    { "scope": ["entity.name.function"], "settings": { "foreground": "#A98BFF", "fontStyle": "italic" } },
    { "scope": ["variable"], "settings": { "foreground": "#E0FFD6" } },
    { "scope": ["string"], "settings": { "foreground": "#B6A6FF" } },
    { "scope": ["comment"], "settings": { "foreground": "#6BA06B", "fontStyle": "italic" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#A98BFF" } },
    { "scope": ["storage.type"], "settings": { "foreground": "#7FFF00", "fontStyle": "bold" } },
    { "scope": ["entity.name.namespace.go"], "settings": { "foreground": "#A98BFF", "fontStyle": "bold" } },
    { "scope": ["entity.name.import.go"], "settings": { "foreground": "#A98BFF", "fontStyle": "bold" } },
    { "scope": ["variable.other.package.go"], "settings": { "foreground": "#A98BFF", "fontStyle": "bold" } },
    { "scope": ["entity.name.import.path.go"], "settings": { "foreground": "#F6E652", "fontStyle": "bold underline" } }
