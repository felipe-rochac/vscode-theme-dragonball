    { "scope": ["variable"], "settings": { "foreground": "#F2F2F2" } },
    { "scope": ["string"], "settings": { "foreground": "#FF2D2D" } },
    { "scope": ["comment"], "settings": { "foreground": "#7C7C8C", "fontStyle": "italic" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#FF2D2D" } },
    { "scope": ["storage.type"], "settings": { "foreground": "#FF2D2D", "fontStyle": "bold" } },
    { "scope": ["entity.name.namespace.go"], "settings": { "foreground": "#FF2D2D", "fontStyle": "bold" } },
    { "scope": ["entity.name.import.go"], "settings": { "foreground": "#FF2D2D", "fontStyle": "bold" } },
    { "scope": ["variable.other.package.go"], "settings": { "foreground": "#7C7C8C", "fontStyle": "bold" } },
    { "scope": ["entity.name.import.path.go"], "settings": { "foreground": "#FF2D2D", "fontStyle": "bold underline" } }
