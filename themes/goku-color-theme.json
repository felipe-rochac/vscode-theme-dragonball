{
      "name": "Dragon Ball Goku",
      "type": "dark",
      "colors": {
        // GitHub Dark base colors
        "editor.background": "#0d1117",
        "editor.foreground": "#fff7e0",
        "editorCursor.foreground": "#FFD700",
        "editor.lineHighlightBackground": "#161b22",
        "editor.selectionBackground": "#264f78",
        "editor.selectionHighlightBackground": "#3b3b3b60",
        "editorIndentGuide.background1": "#21262d",
        "editorIndentGuide.activeBackground1": "#FFD700",
        "editorLineNumber.foreground": "#A67C52",
        "editorLineNumber.activeForeground": "#FFD700",
        "activityBar.background": "#161b22",
        "activityBar.foreground": "#fff7e0",
        "activityBar.activeBorder": "#F47C2C",
        "activityBar.inactiveForeground": "#6e7681",
        "activityBarBadge.background": "#F47C2C",
        "activityBarBadge.foreground": "#161b22",
        "sideBar.background": "#161b22",
        "sideBar.foreground": "#fff7e0",
        "sideBarSectionHeader.background": "#21262d",
        "sideBarSectionHeader.foreground": "#F47C2C",
        "statusBar.background": "#161b22",
        "statusBar.foreground": "#fff7e0",
        "statusBar.noFolderBackground": "#161b22",
        "statusBar.debuggingBackground": "#FFD700",
        "statusBar.debuggingForeground": "#161b22",
        "titleBar.activeBackground": "#161b22",
        "titleBar.activeForeground": "#fff7e0",
        "titleBar.inactiveBackground": "#0d1117",
        "titleBar.inactiveForeground": "#6e7681",
        "tab.activeBackground": "#21262d",
        "tab.activeForeground": "#F47C2C",
        "tab.inactiveBackground": "#161b22",
        "tab.inactiveForeground": "#A67C52",
        "tab.border": "#21262d",
        "tab.activeBorder": "#F47C2C",
        "tab.unfocusedActiveBorder": "#FFD70099",
        "panel.background": "#161b22",
        "panel.border": "#FFD700",
        "editorGroupHeader.tabsBackground": "#161b22"
      },
      "tokenColors": [
        // GitHub Dark base for general code
        { "scope": ["comment", "comment.line", "comment.block"], "settings": { "foreground": "#A67C52", "fontStyle": "italic" } },
        { "scope": ["keyword", "keyword.control.go"], "settings": { "foreground": "#F47C2C", "fontStyle": "bold" } },
        { "scope": ["entity.name.function", "entity.name.function.go"], "settings": { "foreground": "#FFD700", "fontStyle": "italic" } },
        { "scope": ["variable", "variable.other.go"], "settings": { "foreground": "#fff7e0" } },
        { "scope": ["string", "string.quoted.double.go", "string.quoted.single.go"], "settings": { "foreground": "#1D4A9B" } },
        { "scope": ["constant.numeric", "constant.numeric.go"], "settings": { "foreground": "#1D4A9B" } },
        { "scope": ["storage.type", "storage.type.go"], "settings": { "foreground": "#F47C2C", "fontStyle": "bold" } },
        // Go imports and packages (TextMate fallback)
        { "scope": ["entity.name.import.go", "variable.other.package.go"], "settings": { "foreground": "#A67C52", "fontStyle": "bold" } },
        { "scope": ["entity.name.import.path.go"], "settings": { "foreground": "#F47C2C", "fontStyle": "bold underline" } },
        // Source Go fallback
        { "scope": ["source.go"], "settings": { "foreground": "#fff7e0" } },
        // YAML
        { "scope": ["entity.name.tag.yaml"], "settings": { "foreground": "#F47C2C", "fontStyle": "bold" } },
        { "scope": ["entity.name.anchor.yaml"], "settings": { "foreground": "#FFD700", "fontStyle": "bold underline" } },
        { "scope": ["entity.name.alias.yaml"], "settings": { "foreground": "#F47C2C", "fontStyle": "bold" } },
        { "scope": ["string.unquoted.plain.out.yaml"], "settings": { "foreground": "#1D4A9B" } },
        { "scope": ["string.quoted.double.yaml", "string.quoted.single.yaml"], "settings": { "foreground": "#FFD700" } },
        { "scope": ["constant.numeric.yaml"], "settings": { "foreground": "#1D4A9B" } },
        { "scope": ["constant.language.boolean.yaml"], "settings": { "foreground": "#F47C2C", "fontStyle": "bold" } },
        { "scope": ["constant.language.null.yaml"], "settings": { "foreground": "#A67C52", "fontStyle": "italic" } },
        { "scope": ["comment.line.number-sign.yaml"], "settings": { "foreground": "#A67C52", "fontStyle": "italic" } },
        // JSON, JS, TS keys and values
        { "scope": ["support.type.property-name.json", "meta.object-literal.key.js", "meta.object-literal.key.ts", "meta.object-literal.key.tsx"], "settings": { "foreground": "#F47C2C", "fontStyle": "bold" } },
        { "scope": ["string.value.json", "string.quoted.double.js", "string.quoted.single.js", "string.quoted.double.ts", "string.quoted.single.ts", "string.quoted.double.tsx", "string.quoted.single.tsx"], "settings": { "foreground": "#1D4A9B" } },
        { "scope": ["constant.numeric.json", "constant.numeric.js", "constant.numeric.ts", "constant.numeric.tsx"], "settings": { "foreground": "#FFD700" } },
        { "scope": ["constant.language.boolean.json", "constant.language.boolean.js", "constant.language.boolean.ts", "constant.language.boolean.tsx"], "settings": { "foreground": "#F47C2C", "fontStyle": "bold" } },
        { "scope": ["constant.language.null.json", "constant.language.null.js", "constant.language.null.ts", "constant.language.null.tsx"], "settings": { "foreground": "#A67C52", "fontStyle": "italic" } },
        { "scope": ["comment.line.double-slash.js", "comment.line.double-slash.ts", "comment.line.double-slash.tsx", "comment.block.documentation.js", "comment.block.documentation.ts", "comment.block.documentation.tsx"], "settings": { "foreground": "#A67C52", "fontStyle": "italic" } },
        { "scope": ["punctuation.separator.key-value.json", "punctuation.separator.key-value.js", "punctuation.separator.key-value.ts", "punctuation.separator.key-value.tsx"], "settings": { "foreground": "#F47C2C" } },
        { "scope": ["punctuation.definition.block.js", "punctuation.definition.block.ts", "punctuation.definition.block.tsx"], "settings": { "foreground": "#FFD700" } }
      ]
    }