        "editorCursor.foreground": "#FFD700",
        "editor.lineHighlightBackground": "#2C2D3D",
        "editor.selectionBackground": "#00CFFF55",
        "editor.selectionHighlightBackground": "#00CFFF33",
        "editorIndentGuide.background1": "#2C2D3D",
        "editorIndentGuide.activeBackground1": "#FFD700",
        "editorLineNumber.foreground": "#89BFD8",
        "editorLineNumber.activeForeground": "#FFD700",
        "activityBar.background": "#00CFFF",
        "activityBar.foreground": "#F0F8FF",
        "activityBar.activeBorder": "#FFD700",
        "activityBar.inactiveForeground": "#F0F8FF99",
        "activityBarBadge.background": "#FFD700",
        "activityBarBadge.foreground": "#1B1C2A",
        "sideBar.background": "#2C2D3D",
        "sideBar.foreground": "#F0F8FF",
        "sideBarSectionHeader.background": "#00CFFF33",
        "sideBarSectionHeader.foreground": "#FFD700",
        "statusBar.background": "#00CFFF",
        "statusBar.foreground": "#1B1C2A",
        "statusBar.noFolderBackground": "#2C2D3D",
        "statusBar.debuggingBackground": "#FFD700",
        "statusBar.debuggingForeground": "#1B1C2A",
        "titleBar.activeBackground": "#00CFFF",
        "titleBar.activeForeground": "#F0F8FF",
        "titleBar.inactiveBackground": "#2C2D3D",
        "titleBar.inactiveForeground": "#F0F8FF99",
        "tab.activeBackground": "#00CFFF",
        "tab.activeForeground": "#FFD700",
        "tab.inactiveBackground": "#2C2D3D",
        "tab.inactiveForeground": "#F0F8FF99",
        "tab.border": "#FFD700",
        "tab.activeBorder": "#FFD700",
        "tab.unfocusedActiveBorder": "#FFD70099",
        "panel.background": "#2C2D3D",
        "panel.border": "#FFD700",
        "editorGroupHeader.tabsBackground": "#2C2D3D"
      },
      "tokenColors": [
        { "scope": ["keyword"], "settings": { "foreground": "#00CFFF", "fontStyle": "bold" } },
        { "scope": ["entity.name.function"], "settings": { "foreground": "#FFD700", "fontStyle": "italic" } },
        { "scope": ["variable"], "settings": { "foreground": "#F0F8FF" } },
        { "scope": ["string"], "settings": { "foreground": "#89D9F0" } },
        { "scope": ["comment"], "settings": { "foreground": "#89BFD8", "fontStyle": "italic" } },
        { "scope": ["constant.numeric"], "settings": { "foreground": "#FFD700" } },
        { "scope": ["storage.type"], "settings": { "foreground": "#00CFFF", "fontStyle": "bold" } },
        { "scope": ["entity.name.namespace.go"], "settings": { "foreground": "#FFD700", "fontStyle": "bold" } },
        { "scope": ["entity.name.import.go"], "settings": { "foreground": "#00CFFF", "fontStyle": "bold" } },
        { "scope": ["variable.other.package.go"], "settings": { "foreground": "#00CFFF", "fontStyle": "bold" } },
        { "scope": ["entity.name.import.path.go"], "settings": { "foreground": "#FFD700", "fontStyle": "bold underline" } }
      ]
    }