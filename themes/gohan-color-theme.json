{
      "name": "Dragon Ball Gohan",
      "type": "dark",
      "tokenColors": [
        { "scope": ["comment", "comment.line", "comment.block"], "settings": { "foreground": "#7C6BA0", "fontStyle": "italic" } },
        { "scope": ["keyword", "keyword.control.go"], "settings": { "foreground": "#A084CA", "fontStyle": "bold" } }, // Purple
        { "scope": ["entity.name.function", "entity.name.function.go"], "settings": { "foreground": "#FFD700", "fontStyle": "italic" } },
        { "scope": ["variable", "variable.other.go"], "settings": { "foreground": "#FFF7F0" } }, // Light
        { "scope": ["string", "string.quoted.double.go", "string.quoted.single.go"], "settings": { "foreground": "#A084CA" } }, // Purple
        { "scope": ["constant.numeric", "constant.numeric.go"], "settings": { "foreground": "#FFD700" } }, // Gold
        { "scope": ["storage.type", "storage.type.go"], "settings": { "foreground": "#A084CA", "fontStyle": "bold" } }, // Purple
        { "scope": ["entity.name.namespace.go"], "settings": { "foreground": "#A084CA", "fontStyle": "bold" } },
        { "scope": ["entity.name.import.go", "variable.other.package.go"], "settings": { "foreground": "#7C6BA0", "fontStyle": "bold" } },
        { "scope": ["entity.name.import.path.go"], "settings": { "foreground": "#FFD700", "fontStyle": "bold underline" } }, // Gold, underlined
        { "scope": ["entity.name.type", "entity.name.type.go"], "settings": { "foreground": "#A084CA", "fontStyle": "bold" } },
        { "scope": ["source.go"], "settings": { "foreground": "#FFF7F0" } }
      ],
      "colors": {
        // GitHub Dark base colors
        "editor.background": "#0d1117",
        "editor.foreground": "#FFF7F0",
        "editorCursor.foreground": "#FFD700",
        "editor.lineHighlightBackground": "#161b22",
        "editor.selectionBackground": "#264f78",
        "editor.selectionHighlightBackground": "#3b3b3b60",
        "editorIndentGuide.background1": "#21262d",
        "editorIndentGuide.activeBackground1": "#FFD700",
        "editorLineNumber.foreground": "#7C6BA0",
        "editorLineNumber.activeForeground": "#FFD700",
        "activityBar.background": "#161b22",
        "activityBar.foreground": "#FFF7F0",
        "activityBar.activeBorder": "#A084CA",
        "activityBar.inactiveForeground": "#6e7681",
        "activityBarBadge.background": "#A084CA",
        "activityBarBadge.foreground": "#161b22",
        "sideBar.background": "#161b22",
        "sideBar.foreground": "#FFF7F0",
        "sideBarSectionHeader.background": "#21262d",
        "sideBarSectionHeader.foreground": "#A084CA",
        "statusBar.background": "#161b22",
        "statusBar.foreground": "#FFF7F0",
        "statusBar.noFolderBackground": "#161b22",
        "statusBar.debuggingBackground": "#FFD700",
        "statusBar.debuggingForeground": "#161b22",
        "titleBar.activeBackground": "#161b22",
        "titleBar.activeForeground": "#FFF7F0",
        "titleBar.inactiveBackground": "#0d1117",
        "titleBar.inactiveForeground": "#6e7681",
        "tab.activeBackground": "#21262d",
        "tab.activeForeground": "#A084CA",
        "tab.inactiveBackground": "#161b22",
        "tab.inactiveForeground": "#7C6BA0",
        "tab.border": "#21262d",
        "tab.activeBorder": "#A084CA",
        "tab.unfocusedActiveBorder": "#FFD70099",
        "panel.background": "#161b22",
        "panel.border": "#A084CA",
        "editorGroupHeader.tabsBackground": "#161b22"
      }
    }