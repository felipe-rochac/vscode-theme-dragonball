{
  "name": "Vegeta",
  "type": "dark",
  "colors": {
    "editor.foreground": "#F8F8F2",
    "editor.background": "#282A36",
    "editorCursor.foreground": "#FF79C6",
    "editor.lineHighlightBackground": "#44475A",
    "editor.selectionBackground": "#6272A4",
    "editor.inactiveSelectionBackground": "#44475A",
    "editorWhitespace.foreground": "#6272A4",
    "editorIndentGuide.background": "#6272A4",
    "editorIndentGuide.activeBackground": "#50FA7B",
    "editorRuler.foreground": "#6272A4",
    "editorBracketMatch.background": "#50FA7B",
    "editorBracketMatch.border": "#FF79C6",
    "terminal.foreground": "#F8F8F2",
    "terminal.background": "#282A36",
    "terminalCursor.foreground": "#FF79C6",
    "terminal.ansiBlack": "#21222C",
    "terminal.ansiRed": "#FF5555",
    "terminal.ansiGreen": "#50FA7B",
    "terminal.ansiYellow": "#F1FA8C",
    "terminal.ansiBlue": "#BD93F9",
    "terminal.ansiMagenta": "#FF79C6",
    "terminal.ansiCyan": "#8BE9FD",
    "terminal.ansiWhite": "#F8F8F2",
    "terminal.ansiBrightBlack": "#6272A4",
    "terminal.ansiBrightRed": "#FF6E6E",
    "terminal.ansiBrightGreen": "#69FF94",
    "terminal.ansiBrightYellow": "#FFFFA5",
    "terminal.ansiBrightBlue": "#D6ACFF",
    "terminal.ansiBrightMagenta": "#FF92DF",
    "terminal.ansiBrightCyan": "#A4FFFF",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    { "scope": ["comment", "keyword"], "settings": { "foreground": "#6272A4" } },
    { "scope": ["variable", "variable.parameter"], "settings": { "foreground": "#50FA7B" } },
    { "scope": ["constant", "entity.name"], "settings": { "foreground": "#BD93F9" } },
    { "scope": ["string", "meta.string"], "settings": { "foreground": "#F1FA8C" } },
    { "scope": ["entity.name.function", "meta.function-call"], "settings": { "foreground": "#FF79C6" } },
    { "scope": ["entity.name.class", "entity.name.type"], "settings": { "foreground": "#FF92DF" } },
    { "scope": ["entity.name.import.path.go"], "settings": { "foreground": "#1976D2", "fontStyle": "bold underline" } }
  ]
}