{
      "name": "Dragon Ball Vegeta",
      "type": "dark",
      "colors": {
        "editor.background": "#0d1117",
        "editor.foreground": "#E0E6F8",
        "editorCursor.foreground": "#1976D2",
        "editor.lineHighlightBackground": "#161b22",
        "editor.selectionBackground": "#264f78",
        "editor.selectionHighlightBackground": "#3b3b3b60",
        "editorIndentGuide.background1": "#21262d",
        "editorIndentGuide.activeBackground1": "#1976D2",
        "editorLineNumber.foreground": "#6B7CA0",
        "editorLineNumber.activeForeground": "#1976D2",
        "activityBar.background": "#161b22",
        "activityBar.foreground": "#E0E6F8",
        "activityBar.activeBorder": "#1976D2",
        "activityBar.inactiveForeground": "#6e7681",
        "activityBarBadge.background": "#1976D2",
        "activityBarBadge.foreground": "#161b22",
        "sideBar.background": "#161b22",
        "sideBar.foreground": "#E0E6F8",
        "sideBarSectionHeader.background": "#21262d",
        "sideBarSectionHeader.foreground": "#1976D2",
        "statusBar.background": "#161b22",
        "statusBar.foreground": "#E0E6F8",
        "statusBar.noFolderBackground": "#161b22",
        "statusBar.debuggingBackground": "#FFD700",
        "statusBar.debuggingForeground": "#161b22",
        "titleBar.activeBackground": "#161b22",
        "titleBar.activeForeground": "#E0E6F8",
        "titleBar.inactiveBackground": "#0d1117",
        "titleBar.inactiveForeground": "#6e7681",
        "tab.activeBackground": "#21262d",
        "tab.activeForeground": "#1976D2",
        "tab.inactiveBackground": "#161b22",
        "tab.inactiveForeground": "#6B7CA0",
        "tab.border": "#21262d",
        "tab.activeBorder": "#1976D2",
        "tab.unfocusedActiveBorder": "#1976D299",
        "panel.background": "#161b22",
        "panel.border": "#1976D2",
        "editorGroupHeader.tabsBackground": "#161b22"
      },
      "tokenColors": [
        // Base for general code
        { "scope": ["comment", "comment.line", "comment.block"], "settings": { "foreground": "#6B7CA0", "fontStyle": "italic" } },
        { "scope": ["keyword", "keyword.control.go"], "settings": { "foreground": "#1976D2", "fontStyle": "bold" } },
        { "scope": ["entity.name.function", "entity.name.function.go"], "settings": { "foreground": "#FFD700", "fontStyle": "italic" } },
        { "scope": ["variable", "variable.other.go"], "settings": { "foreground": "#E0E6F8" } },
        { "scope": ["string", "string.quoted.double.go", "string.quoted.single.go"], "settings": { "foreground": "#3B4D9B" } },
        { "scope": ["constant.numeric", "constant.numeric.go"], "settings": { "foreground": "#FFD700" } },
        { "scope": ["storage.type", "storage.type.go"], "settings": { "foreground": "#1976D2", "fontStyle": "bold" } },
        { "scope": ["entity.name.type", "entity.name.type.go"], "settings": { "foreground": "#A084CA", "fontStyle": "bold" } },
        // Go imports and packages (TextMate fallback)
        { "scope": ["entity.name.import.go", "variable.other.package.go"], "settings": { "foreground": "#6B7CA0", "fontStyle": "bold" } },
        { "scope": ["entity.name.import.path.go"], "settings": { "foreground": "#FFD700", "fontStyle": "bold underline" } },
        // Source Go fallback
        { "scope": ["source.go"], "settings": { "foreground": "#E0E6F8" } },
        // YAML
        { "scope": ["entity.name.tag.yaml"], "settings": { "foreground": "#1976D2", "fontStyle": "bold" } },
        { "scope": ["entity.name.anchor.yaml"], "settings": { "foreground": "#FFD700", "fontStyle": "bold underline" } },
        { "scope": ["entity.name.alias.yaml"], "settings": { "foreground": "#1976D2", "fontStyle": "bold" } },
        { "scope": ["string.unquoted.plain.out.yaml"], "settings": { "foreground": "#3B4D9B" } },
        { "scope": ["string.quoted.double.yaml", "string.quoted.single.yaml"], "settings": { "foreground": "#FFD700" } },
        { "scope": ["constant.numeric.yaml"], "settings": { "foreground": "#3B4D9B" } },
        { "scope": ["constant.language.boolean.yaml"], "settings": { "foreground": "#1976D2", "fontStyle": "bold" } },
        { "scope": ["constant.language.null.yaml"], "settings": { "foreground": "#6B7CA0", "fontStyle": "italic" } },
        { "scope": ["comment.line.number-sign.yaml"], "settings": { "foreground": "#6B7CA0", "fontStyle": "italic" } },
        // JSON, JS, TS keys and values
        { "scope": ["support.type.property-name.json", "meta.object-literal.key.js", "meta.object-literal.key.ts", "meta.object-literal.key.tsx"], "settings": { "foreground": "#1976D2", "fontStyle": "bold" } },
        { "scope": ["string.value.json", "string.quoted.double.js", "string.quoted.single.js", "string.quoted.double.ts", "string.quoted.single.ts", "string.quoted.double.tsx", "string.quoted.single.tsx"], "settings": { "foreground": "#3B4D9B" } },
        { "scope": ["constant.numeric.json", "constant.numeric.js", "constant.numeric.ts", "constant.numeric.tsx"], "settings": { "foreground": "#FFD700" } },
        { "scope": ["constant.language.boolean.json", "constant.language.boolean.js", "constant.language.boolean.ts", "constant.language.boolean.tsx"], "settings": { "foreground": "#1976D2", "fontStyle": "bold" } },
        { "scope": ["constant.language.null.json", "constant.language.null.js", "constant.language.null.ts", "constant.language.null.tsx"], "settings": { "foreground": "#6B7CA0", "fontStyle": "italic" } },
        { "scope": ["comment.line.double-slash.js", "comment.line.double-slash.ts", "comment.line.double-slash.tsx", "comment.block.documentation.js", "comment.block.documentation.ts", "comment.block.documentation.tsx"], "settings": { "foreground": "#6B7CA0", "fontStyle": "italic" } },
        { "scope": ["punctuation.separator.key-value.json", "punctuation.separator.key-value.js", "punctuation.separator.key-value.ts", "punctuation.separator.key-value.tsx"], "settings": { "foreground": "#1976D2" } },
        { "scope": ["punctuation.definition.block.js", "punctuation.definition.block.ts", "punctuation.definition.block.tsx"], "settings": { "foreground": "#FFD700" } },
        // Makefile
        { "scope": ["keyword.control.makefile"], "settings": { "foreground": "#FFD700", "fontStyle": "bold" } },
        { "scope": ["variable.assignment.makefile"], "settings": { "foreground": "#1976D2", "fontStyle": "bold" } },
        { "scope": ["entity.name.function.target.makefile"], "settings": { "foreground": "#A084CA", "fontStyle": "bold underline" } },
        { "scope": ["string.quoted.double.makefile", "string.quoted.single.makefile"], "settings": { "foreground": "#3B4D9B" } },
        { "scope": ["comment.line.number-sign.makefile"], "settings": { "foreground": "#6B7CA0", "fontStyle": "italic" } },
        { "scope": ["meta.scope.target.makefile"], "settings": { "foreground": "#A084CA", "fontStyle": "bold" } },
        { "scope": ["meta.scope.prerequisites.makefile"], "settings": { "foreground": "#A084CA", "fontStyle": "" } },
        // Terraform
        { "scope": ["keyword.control.terraform"], "settings": { "foreground": "#1976D2", "fontStyle": "bold" } },
        { "scope": ["variable.other.terraform"], "settings": { "foreground": "#E0E6F8" } },
        { "scope": ["entity.name.function.terraform"], "settings": { "foreground": "#A084CA", "fontStyle": "italic" } },
        { "scope": ["entity.name.type.terraform"], "settings": { "foreground": "#A084CA", "fontStyle": "bold" } },
        { "scope": ["string.quoted.double.terraform", "string.quoted.single.terraform"], "settings": { "foreground": "#3B4D9B" } },
        { "scope": ["constant.numeric.terraform"], "settings": { "foreground": "#FFD700" } },
        { "scope": ["constant.language.boolean.terraform"], "settings": { "foreground": "#1976D2", "fontStyle": "bold" } },
        { "scope": ["comment.line.number-sign.terraform"], "settings": { "foreground": "#8b949e", "fontStyle": "italic" } },
        { "scope": ["support.constant.terraform"], "settings": { "foreground": "#FFD700", "fontStyle": "bold" } },
        { "scope": ["punctuation.definition.block.terraform"], "settings": { "foreground": "#A084CA" } },
        { "scope": ["support.function.builtin.terraform"], "settings": { "foreground": "#A084CA", "fontStyle": "bold italic" } },
        { "scope": ["variable.other.readwrite.terraform"], "settings": { "foreground": "#A084CA", "fontStyle": "italic" } }
      ]
    }